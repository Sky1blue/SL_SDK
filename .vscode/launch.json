    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceFolder}/example/armgcc_stm32h7_demo/build/cross/none/debug/stm32h7_demo",
            "name": "Debug Microcontroller by stlink",
            "request": "launch",
            "type": "cortex-debug",
            "svdFile": "${workspaceFolder}/example/armgcc_stm32h7_demo/STM32H750x.svd",
            "showDevDebugOutput": false,
            "servertype": "openocd",
            "configFiles": [
                "${workspaceFolder}/example/armgcc_stm32h7_demo/debug.cfg"
            ]
        },
        {
            "name": "(gdb for riscv_ubuntu) 启动",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/example/riscvgcc_k210_demo/build/cross/none/debug/xdemo_riscv64",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "/home/administrator/.local/bin/riscv_64bit_toolchain/bin/riscv64-unknown-elf-gdb",
            "miDebuggerServerAddress": "localhost:1234",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb for arm_windows) 启动",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/example/armgcc_stm32h7_demo/build/cross/none/debug/stm32h7_demo",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/UserProfession/GNU Arm Embedded Toolchain/10 2021.10/bin/arm-none-eabi-gdb.exe",
            "miDebuggerServerAddress": "localhost:3333",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}